		// Language agnostic section
		//-----------------------------------------------------------------------------
		{
			"name": "Language agnostic: class",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"keyword.control.class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#880E4F"
			}
		},
		{
			"name": "Language agnostic: comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#009688",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Language agnostic: constant",
			"scope": [
				"constant.language",
